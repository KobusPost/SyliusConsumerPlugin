<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <import resource="services/association_type.xml" />
        <import resource="services/attribute.xml" />
        <import resource="services/attribute_option.xml" />
        <import resource="services/family.xml" />
        <import resource="services/group.xml" />
        <import resource="services/product.xml" />
        <import resource="services/taxon.xml" />
    </imports>

    <services>
        <service class="Sylake\SyliusConsumerPlugin\Command\SetupCommand">
            <argument type="service" id="sylius.repository.product_attribute" />
            <argument type="service" id="sylius.factory.product_attribute" />
            <tag name="console.command" />
        </service>
    </services>
</container>
